<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mscorlib="clr-namespace:System;assembly=mscorlib"
    xmlns:imaging="clr-namespace:System.Windows.Media.Imaging;assembly=System.Windows">

    <BitmapImage x:Key="BackgroundImage" UriSource="../Images/Light/Background.png"/>
    <ImageBrush x:Key="BackgroundBrush" Stretch="None" AlignmentX="Left" ImageSource="{StaticResource BackgroundImage}"/>
    
    <BitmapImage x:Key="DotsImage" UriSource="../Images/Light/Dots.png"/>
    <BitmapImage x:Key="DotsNavigatorImage" UriSource="../Images/Light/DotsNavigator.png"/>
    <BitmapImage x:Key="TelerikLogo" UriSource="../Images/Light/Telerik.png"/>

    <BitmapImage x:Key="NavigationExpander" UriSource="../Images/Light/NavigationExpander.png"/>
    
    <SolidColorBrush x:Key="TemplateNavigationControlBackground" Color="#CECECE"/>
    
</ResourceDictionary>